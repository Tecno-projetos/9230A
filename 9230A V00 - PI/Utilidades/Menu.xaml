    <Grid Background="#FF5A5A5A">
        <Image HorizontalAlignment="Left" Height="60" VerticalAlignment="Top" Width="195" Source="/9230A V00 - PI;component/Imagens/Logo_Automasul.png" Margin="-13,0,0,0"/>
        <Image HorizontalAlignment="Left" Height="78" VerticalAlignment="Top" Width="172" Source="/9230A V00 - PI;component/Imagens/beckerA.png" Margin="5,52,0,0"/>

        <Button x:Name="BT_Time_Scan" HorizontalAlignment="Left" Height="117.833" Margin="181.749,3.498,0,0" VerticalAlignment="Top" Width="100" Background="{x:Null}" Foreground="Black" Cursor="Hand" IsTabStop="False" TabIndex="1"   RenderTransformOrigin="0.5,0.5" ToolTip="Tempo CLP">
            <Button.RenderTransform>
                <TransformGroup>
                    <ScaleTransform ScaleX="{Binding Scala_GS, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type local:Menu}}}" ScaleY="{Binding Scala_GS, RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type local:Menu}}}"/>
                    <SkewTransform/>
                    <RotateTransform/>
                    <TranslateTransform/>
                </TransformGroup>
            </Button.RenderTransform>
            <Button.BorderBrush>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="#FFD1D1D1" Offset="0.007"/>
                    <GradientStop Color="White" Offset="1"/>
                </LinearGradientBrush>
            </Button.BorderBrush>
            <StackPanel Orientation="Vertical">
                <Image Source="/9230A V00 - PI;component/Imagens/Timer_Scan.png" HorizontalAlignment="Center" VerticalAlignment="Center" Width="90" Height="95" Stretch="Uniform"/>
                <TextBlock TextWrapping="Wrap" TextAlignment="Center" FontSize="14" Foreground="White" RenderTransformOrigin="0.5,0.5">
                    <TextBlock.RenderTransform>
                        <TransformGroup>
                            <ScaleTransform/>
                            <SkewTransform/>
                            <RotateTransform/>
                            <TranslateTransform/>
                        </TransformGroup>
                    </TextBlock.RenderTransform><Run Text="Timer Scan"  /></TextBlock>
            </StackPanel>
        </Button>
